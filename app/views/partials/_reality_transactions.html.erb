<%=render 'partials/zone-time'%>
<%unless ultima_page%>
    <%@transactions.each do |transaction|%>
        <%=render 'partials/transaction', transaction: transaction%>
    <%end%>
<%else%>
    <%if @transactions_not_paginate.present?%>
        <%@transactions_not_paginate.each do |transaction|%>
            <%=render 'partials/transaction', transaction: transaction%>
        <%end%>
    <%end%>
<%end%>

