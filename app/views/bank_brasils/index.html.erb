<p id="notice"><%= notice %></p>

<h1>Bank Brasils</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Last name</th>
      <th>Country</th>
      <th>Cpf</th>
      <th>Number agency</th>
      <th>Number account</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bank_brasils.each do |bank_brasil| %>
      <tr>
        <td><%= bank_brasil.name %></td>
        <td><%= bank_brasil.last_name %></td>
        <td><%= bank_brasil.country %></td>
        <td><%= bank_brasil.document %></td>
        <td><%= bank_brasil.number_agency %></td>
        <td><%= bank_brasil.number_account %></td>
        <td><%= link_to 'Show', bank_brasil %></td>
        <td><%= link_to 'Edit', edit_bank_brasil_path(bank_brasil) %></td>
        <td><%= link_to 'Destroy', bank_brasil, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bank Brasil', new_bank_brasil_path %>
